class Solution {

    public ArrayList<Integer> missingRange(int[] arr, int low, int high) {

        // code here

        Arrays.sort(arr);

        ArrayList<Integer> list= new ArrayList<>();

        for(int i=low;i<=high;i++){

            if(Arrays.binarySearch(arr,i)<0){

                list.add(i);

            }

        }

        return list;

    }

}
